MapTransitionTrigger from_PokeCenter_to_Tom_Town_at_PokeCenter01 {
	condition: MapGlobal_PreviousMap_Tom_Town&MapGlobal_toPokeCenterFromEntrance_PokeCenter01
	global: !MapGlobal_toPokeCenterFromEntrance_PokeCenter01
	nextMap: Tom_Town
	mapEntrance: PokeCenter01
}

MapTransitionTrigger from_PokeCenter_to_RSATown_at_PokeCenter01 {
	condition: MapGlobal_PreviousMap_RSATown&MapGlobal_toPokeCenterFromEntrance_PokeCenter01
	global: !MapGlobal_toPokeCenterFromEntrance_PokeCenter01
	nextMap: RSATown
	mapEntrance: PokeCenter01
}

